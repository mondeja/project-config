{
  rules: [
    {
      files: ["package.json"],
      crossJMESPathsMatch: [
        [
          "license",
          [
            "gh://spdx/license-list-data@v3.17/json/licenses.json",
            "licenses[?isOsiApproved] | [?isDeprecatedLicenseId == `false`].licenseId",
          ],
          "contains([1], [0])",
          true,
        ],
      ],
    },
  ],
}
